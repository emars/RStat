{
  "manifest_version": 2,
  "name": "RStat",
  "description": "Reddit Statistics collecter for rstat.emaf.ca",
  "version":"0.1",
  "permissions": [
    "http://rstat.emaf.ca",
    "storage"
  ],
  "content_scripts":[
    {
      "matches": ["http://www.reddit.com/*"],
      "js": ["jquery.js","main.js"]
    }
  ]
}
